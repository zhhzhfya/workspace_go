		<istmp>true</istmp>
		<addType></addType>
	</info>
	<info>
		<name>微信</name>
		<source>/opt/trans-data/wechat/wj_wechat_trace</source>
		<dest>/opt/flume-data/wechat/wj_wechat_trace</dest>
		<sub_dir></sub_dir>
		<istmp>true</istmp>
		<addType></addType>
	</info>
	<info>
		<name>微信头像</name>
		<source>/opt/trans-data/wechat/wj_pic_head</source>
		<dest>/opt/windows-data/wechat-file/WJ_RESOURCE/WECHAT/HEAD_PIC</dest>
		<sub_dir></sub_dir>
		<istmp></istmp>
		<addType></addType>
	</info>
