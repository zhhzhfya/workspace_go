<infos>
	<sleep_time>2000</sleep_time>
	<info>
		<name>网吧的信息</name>
		<source>/opt/trans-data/net/wj_net</source>
		<dest>/opt/flume-data/net/wj_net</dest>
		<sub_dir></sub_dir>
		<istmp>true</istmp>
		<addType></addType>
		<sleep>3000</sleep>
	</info>
	<info>
		<name>网吧聊天</name>
		<source>/opt/trans-data/net/wj_net_chat</source>
		<dest>/opt/flume-data/net/wj_net_chat</dest>
		<sub_dir></sub_dir>
		<istmp>true</istmp>
		<addType></addType>
	</info>
	<info>
		<name>网吧的上下机</name>
		<source>/opt/trans-data/net/wj_net_use</source>
		<dest>/opt/flume-data/net/wj_net_use</dest>
		<sub_dir></sub_dir>
		<istmp>true</istmp>
		<addType></addType>
	</info>
	<info>
		<name>网吧注册登记</name>
		<source>/opt/trans-data/net/wj_net_regist</source>
		<dest>/opt/flume-data/net/wj_net_regist</dest>
		<sub_dir></sub_dir>
		<istmp>true</istmp>
		<addType></addType>
	</info>
	<info>
		<name>上机照片</name>
		<source>/opt/trans-data/net-user-pic/use_pic</source>
		<dest>/opt/windows-data/video-file/WJ_RESOURCE/NET/NET_USE_PIC</dest>
		<sub_dir>/2:0-4/2:4-6/2:6-8/</sub_dir>
		<istmp></istmp>
		<addType></addType>
	</info>
	<info>
		<name>身份证照片</name>
		<source>/opt/trans-data/net-user-pic/idcard_pic</source>
		<dest>/opt/windows-data/video-file/WJ_RESOURCE/ID_CARD</dest>
		<sub_dir>/0:0-4/</sub_dir>
		<istmp></istmp>
		<addType></addType>
	</info>
	<info>
		<name>网吧的上网轨迹</name>
		<source>/opt/trans-data/net/wj_net_trace</source>
		<dest>/opt/flume-data/net/wj_net_trace</dest>
		<sub_dir></sub_dir>
		<istmp>true</istmp>
		<addType></addType>
	</info>
	<info>
		<name>微信</name>
		<source>/opt/trans-data/wechat/wj_wechat_info</source>
		<dest>/opt/flume-data/wechat/wj_wechat_info</dest>
		<sub_dir></sub_dir>
		<istmp>true</istmp>
		<addType></addType>
	</info>
	<info>
		<name>微信朋友圈图片</name>
		<source>/opt/trans-data/wechat/wj_pic_moment</source>
		<dest>/opt/windows-data/wechat-file/WJ_RESOURCE/WECHAT/MOMENT_PIC</dest>
		<sub_dir></sub_dir>
		<istmp></istmp>
		<addType></addType>
		<sleep>5000</sleep>
	</info>
	<info>
		<name>微信</name>
		<source>/opt/trans-data/wechat/wj_wechat_moment</source>
		<dest>/opt/flume-data/wechat/wj_wechat_moment</dest>
		<sub_dir></sub_dir>
		<istmp>true</istmp>
		<addType></addType>
